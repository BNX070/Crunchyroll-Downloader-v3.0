    $(document).ready(function () {
      var $commentsWrapper = $('.guestbook.comments');
      $commentsWrapper.on('click', '.comment-reply', function (e) {
        e.preventDefault();
        e.stopPropagation();
        var $this = $(this),
          id = $this.attr('id').replace('comment-reply_', ''),
          replyList = $('#replylist_' + id),
          reply = $('#reply_' + id);

        if (replyList.is(':hidden')) {
          replyList.show(200);
        } else {
          reply.show(200);
        }

        $this.hide();
        var word = l('generic.sentencecase.hide_replies', 'Hide replies');
        if (replyList.children('.reply').length == 1) {
          word = l('generic.sentencecase.hide_reply', 'Hide reply');
        }

        $('#comment-showhide_' + id).text(word).show(100);
      });

      $commentsWrapper.on('click', '.comment-showhide', function (e) {
        e.preventDefault();
        e.stopPropagation();
        var $this = $(this),
          id = $this.attr('id').replace('comment-showhide_', ''),
          replyList = $('#replylist_' + id),
          reply = $('#comment-reply_' + id);

        if (!replyList.is(':hidden')) {
          replyList.hide();
          $(this).hide();
          reply.show(100);
          var len = replyList.children('.reply').length;
          if (len == 1) {
            word = l('generic.sentencecase.show_reply', 'Show reply');
          } else {
            word = l('generic.sentencecase.show__x__replies', 'Show {x} replies', null, {'x': len});
          }
          reply.text(word).show(100);
        }
      })
    });
  </script>

